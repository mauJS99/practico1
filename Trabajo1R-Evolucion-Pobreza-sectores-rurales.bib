  title = {La Reconfiguraci{\'o}n Del Mapa de La Pobreza Multidimensional En {{Chile}}},
  author = {{Rodr{\'i}guez-Garc{\'e}s}, Carlos Ren{\'e} and {Mu{\~n}oz-Soto}, Johana and {Padilla-Fuentes}, Geraldo},
